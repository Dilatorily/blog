{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "jest": true
  },
  "globals": {
    "__DEV__": false
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": ["error", {
    components: ["Link"],
    specialLink: ["to"],
    aspects: ["noHref", "invalidHref", "preferButton"]
    }]
  }
}
